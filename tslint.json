{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single", "jsx-double"],
      "semicolon": [true, "never"],
      "interface-name": [true, "never-prefix"]
    },
    "rulesDirectory": [],
    "exclude": [
      "node_modules"
    ],
    "compilerOptions": {
      "moduleResolution": "node"
    }
}