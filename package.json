{
  "name": "letter-paper",
  "version": "0.0.1",
  "description": "personal letter-paper style blog site.",
  "main": "src/app.js",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "build": "webpack --config ./scripts/webpack.prod.js",
    "dev": "webpack-dev-server  --hot --open --config ./scripts/webpack.dev.js",
    "lint": "eslint . --ext .js,.vue -f table --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yidafu/letter-paper.git"
  },
